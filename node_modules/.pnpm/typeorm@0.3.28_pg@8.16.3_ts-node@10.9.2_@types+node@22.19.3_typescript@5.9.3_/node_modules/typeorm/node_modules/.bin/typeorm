#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/pyowonsik/Downloads/workspace/gear_freak_nestjs/node_modules/.pnpm/typeorm@0.3.28_pg@8.16.3_ts-node@10.9.2_@types+node@22.19.3_typescript@5.9.3_/node_modules/typeorm/node_modules:/Users/pyowonsik/Downloads/workspace/gear_freak_nestjs/node_modules/.pnpm/typeorm@0.3.28_pg@8.16.3_ts-node@10.9.2_@types+node@22.19.3_typescript@5.9.3_/node_modules:/Users/pyowonsik/Downloads/workspace/gear_freak_nestjs/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/pyowonsik/Downloads/workspace/gear_freak_nestjs/node_modules/.pnpm/typeorm@0.3.28_pg@8.16.3_ts-node@10.9.2_@types+node@22.19.3_typescript@5.9.3_/node_modules/typeorm/node_modules:/Users/pyowonsik/Downloads/workspace/gear_freak_nestjs/node_modules/.pnpm/typeorm@0.3.28_pg@8.16.3_ts-node@10.9.2_@types+node@22.19.3_typescript@5.9.3_/node_modules:/Users/pyowonsik/Downloads/workspace/gear_freak_nestjs/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
